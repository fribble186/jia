/* pages/settings/settings.wxss */
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.backicon{
  margin:20rpx;
  width:90rpx;
  height:90rpx;
}
.paper{
  position: absolute;
  bottom:20rpx;
  right:180rpx;
  width:70rpx;
  height:90rpx;
}
.can{
  position: absolute;
  bottom:20rpx;
  right:30rpx;
  width:90rpx;
  height:90rpx;
}
.circle{
  background: #f1cee6;
  width:100%;
  height:200rpx;
  margin-bottom: 30rpx;
}
.menu{
  background: #fdffe2;
  width:100%;
  height:450rpx;
  margin-bottom: 30rpx;
}
.dish{
  background: #d8f9cf;
  width:100%;
  height:300rpx;
}
.title{
  font-weight: 600;
  text-align: center;
  font-size: 32rpx;
}
.c_members{
  width:100%;
  height:135rpx;
  white-space: nowrap;
  padding-top:10rpx;
  padding-left:10rpx;
}
.c_member{
  width:115rpx;
  height:115rpx;
  border:1rpx black solid;
  margin-right:20rpx;
  display: inline-block;
  border-radius: 115rpx;
}
.m_members{
  width:100%;
  height:390rpx;
  white-space: nowrap;
  padding-top:10rpx;
  padding-left:10rpx;
}
.m_member{
  width:220rpx;
  height:370rpx;
  border:1rpx black solid;
  margin-right:20rpx;
  display: inline-block;
  line-height: 390rpx;
  margin-bottom:-300rpx;
  writing-mode: vertical-lr;
  font-weight: 900;
  font-size: 55rpx;
}

.m_member text{
  margin:30rpx;
  padding-right:90rpx;
}

.btn{
  border-radius: 300rpx;
  color:white;
  background-color: black;
  opacity: 0.7;
  font-weight: 900;
  width:550rpx;
  margin-top:60rpx;
}